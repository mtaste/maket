<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration>
	<!-- 设置Mysql驱动路径 -->
	<classPathEntry location="./ojdbc14.jar" />
	<!-- 此处指定生成针对MyBatis3的DAO -->
	<context id="shark" targetRuntime="MyBatis3">
		<!-- jdbc连接信息 -->
		<jdbcConnection driverClass="oracle.jdbc.driver.OracleDriver"
			connectionURL="jdbc:oracle:thin:@192.168.1.246:1521:orcl" userId="api"
			password="api" />
		<javaModelGenerator targetPackage="com.plat.api.entity"
			targetProject="../src/main/java" />
		<sqlMapGenerator targetPackage="com.plat.api.dao.mybatis.sqlMap"
			targetProject="../src/main/java" />
		<javaClientGenerator targetPackage="com.plat.api.dao.mybatis.mapper"
			targetProject="../src/main/java" type="XMLMAPPER" />

		<table tableName="API_TEC_ABILITY" domainObjectName="ApiTecAbility">
			<columnOverride column="CREATE_TIME" jdbcType="TIMESTAMP" />
		</table>
	</context>

</generatorConfiguration>